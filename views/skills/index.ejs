<!-- views/skills/index.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Skills</title>
</head>
<body>
  <h1>Skills</h1>
  <ul>
    <% skills.forEach(function(s) { %>
      <li><a href="/skill/<%= s.id %>"><%= s.name %> - <%= s.level %></a></li>
    <% }); %>
  </ul>
</body>
</html>
